<h1>管理后台－用户列表</h1>
<div class="row">
  <div class="col-sm-12">
    <form action="<%= search_admin_users_path %>" method="get">
      用户名: <%= text_field_tag :username,params[:username]%><%=submit_tag 'Submit' %>
    </form>
    <table class="table">
      <tr>
        <th>ID</th>
        <th>用户名</th>
        <th></th>
      </tr>
      <%if false%>
          <%@users.each do |user|%>
            <%= render partial: 'one_user', locals: {user:user}%>
          <%end%>
      <%end%>
      <%= render partial: 'one_user', collection: @users,as: :user%>
    </table>
    <%= will_paginate @users%>
  </div>
</div>